{
  "name": "@unction/groupby",
  "version": "10.10.1",
  "description": "Creates a record tree where the key is a computation on the value and the value is a list of the values that match with that computation",
  "keywords": [
    "unction",
    "functional"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/unctionjs/groupBy#readme",
  "repository": "github:unctionjs/groupBy",
  "bugs": "https://github.com/unctionjs/groupBy/issues",
  "contributors": [
    {
      "name": "Kurtis Rainbolt-Greene",
      "email": "kurtis@rainbolt-greene.online",
      "url": "https://kurtis.rainbolt-greene.online"
    }
  ],
  "main": "main.js",
  "module": "module.js",
  "browser": "browser.js",
  "types": "types.d.ts",
  "files": [
    "index.ts",
    "main.js",
    "browser.js",
    "types/*.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "BABEL_ENV=test jest --coverage",
    "build": "npm run build:browser && npm run build:main",
    "build:main": "babel --env-name main --out-file './main.js' './index.ts'",
    "build:browser": "babel --env-name browser --out-file './browser.js' './index.ts'",
    "lint": "eslint './index.ts'",
    "typecheck": "tsc --noEmit",
    "ci": "npm run typecheck && npm run lint && npm run test"
  },
  "devDependencies": {
    "@babel/cli": "7.x",
    "@babel/core": "7.x",
    "@babel/plugin-proposal-object-rest-spread": "7.x",
    "@babel/plugin-syntax-dynamic-import": "7.x",
    "@babel/preset-env": "7.x",
    "@babel/preset-typescript": "7.x",
    "@babel/register": "7.x",
    "@types/jest": "24.x",
    "@types/node": "12.x",
    "babel-eslint": "10.x",
    "babel-jest": "24.x",
    "babel-plugin-annotate-console-log": "1.x",
    "babel-plugin-module-resolver": "3.x",
    "babel-preset-minify": "0.5.x",
    "eslint": "5.x",
    "eslint-import-resolver-babel-module": "5.x",
    "eslint-plugin-babel": "5.x",
    "eslint-plugin-jest": "22.x",
    "eslint-plugin-promise": "4.x",
    "jest": "24.x",
    "jest-extended": "0.11.x",
    "typescript": "3.x"
  },
  "dependencies": {
    "@unction/fresh": "8.9.1",
    "@unction/get": "2.10.0",
    "@unction/mergeright": "7.10.0",
    "@unction/of": "5.10.0",
    "@unction/reducevalues": "8.10.0"
  }
}
